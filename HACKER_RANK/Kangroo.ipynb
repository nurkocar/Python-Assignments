    "def kangaroo(x1, v1, x2, v2):\n",
    "    while True:\n",
    "        if x1 < x2 and v1 < v2:\n",
    "            return 'No'\n",
    "        else:\n",
    "            x1 += v1\n",
    "            x2 += v2\n",
    "            if x1 == x2:\n",
    "                return 'Yes'\n",
    "                break"
   ]
